{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 70
    },
    "Description": {
      "type": "text",
      "required": true,
      "maxLength": 250
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "services"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "service"
    },
    "Enabled": {
      "type": "boolean",
      "required": true
    }
  }
}
