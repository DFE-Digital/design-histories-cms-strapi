{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "maxLength": 70
    },
    "Lede": {
      "type": "text",
      "required": true,
      "maxLength": 250
    },
    "Body": {
      "type": "richtext",
      "required": true
    },
    "Publication_date_override": {
      "type": "date"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "posts"
    }
  }
}
